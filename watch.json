{
    "install": {
        "include": [
            "^package\\.json$",
            "^\\.env$"
        ]
    },
    "restart": {
        "exclude": [
            "\\.markdown",
            "\\.md",
            "\\.mdown"
        ],
        "include": [
            "^assets/",
            "\\.js$",
            "\\.json"
        ]
    },
    "throttle": 120000
}
